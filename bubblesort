#include<iostream>
using namespace std;
int bubblesort(int arr[],int len)
{
for (int i = 0; i < len-1; i++)
{
    for (int j = 0; j < len-i-1; j++)
    {
        if (arr[j]>arr[j+1])
        {
            swap(arr[j],arr[j+1]);

        }
        
    }
    
}  
for (int i = 0; i <len; i++)
{
    cout<<arr[i]<<endl;
}

}
int main(){
    int arr[]={10,40,30,20,50,6};
    int len=sizeof(arr)/sizeof(arr[0]);
    bubblesort(arr,len);

}
